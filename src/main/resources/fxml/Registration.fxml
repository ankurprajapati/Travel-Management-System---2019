<AnchorPane id="AnchorPane" prefHeight="615.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.RegistrationController">
  <children>
    <Label layoutX="50.0" layoutY="34.0" text="CLIENT REGISTRATION">
      <font>
        <Font name="Courier New Bold" size="25.0" />
