    <GridPane layoutX="74.0" layoutY="102.0" prefHeight="325.0" prefWidth="452.0">
      <children>
        <Label text="FIRST NAME" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <font>
            <Font name="Courier New" size="16.0" fx:id="x1" />
          </font>
        </Label>
        <Label font="$x1" text="LAST NAME" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <Label font="$x1" text="BIRTH DATE" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Label font="$x1" text="EMAIL ADDRESS" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <Label font="$x1" text="PHONE" GridPane.columnIndex="0" GridPane.rowIndex="4" />
        <Label font="$x1" text="PASSWORD" GridPane.columnIndex="0" GridPane.rowIndex="5" />
        <Label font="$x1" text="CONFIRM PASSWORD" GridPane.columnIndex="0" GridPane.rowIndex="6" />
        <TextField fx:id="fname" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <TextField fx:id="lname" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <DatePicker fx:id="datepicker" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="email" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
        <TextField fx:id="phone" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
        <PasswordField fx:id="password" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        <PasswordField fx:id="confirmpassword" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="225.0" minWidth="10.0" prefWidth="177.0" />
